<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    
<hibernate-mapping>
	<class name="cn.itcast.manyToMany.StudentModel" table="t_student">
		<!--   <cache usage="read-only"/>   -->
		<id name = "id">
			<generator class="native"/>
		</id>
		
		<property name="name"/>
		
		 
		 <set name="tms" table="s_t" >
		 	<!-- <cache usage="read-only"/> -->
		 	<key column="s_id"/>
		 <many-to-many 
		 	column="t_id"
		    class="cn.itcast.manyToMany.TeacherModel"
		 />
		 </set>
		
	</class>

</hibernate-mapping>